{
  "name": "dashboard-layouts",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/dashboard/layouts/src",
  "prefix": "buildmotion",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/dashboard/layouts"
      ],
      "options": {
        "jestConfig": "libs/dashboard/layouts/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/dashboard/layouts/**/*.ts",
          "libs/dashboard/layouts/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
